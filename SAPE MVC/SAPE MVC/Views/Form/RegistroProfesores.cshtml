@{
    ViewBag.Title = "RegistroProfesores";
    Layout = "~/Views/Shared/_FormLayout.cshtml";
}

@section Scripts {
    <script src="/Content/Scripts/form-RegistroProfesores.js"></script>
}


<div class="main-form container">
    <h2>Registro de Profesores</h2>
    <form role="form" method="post">
        <div class="form-group">
            <label for="nombreProfesor">Nombre</label>
            <input id="nombreProfesor" class="form-control" type="text" name="nombreProfesor" />
        </div>
        <div class="form-group">
            <label for="prmApeProf">Primer apellido</label>
            <input id="prmApeProf" class="form-control" type="text" name="pmrApeProf" />
        </div>
        <div class="form-group">
            <label for="sdoApeProf">Segundo apellido</label>
            <input id="sdoApeProf" class="form-control" type="text" name="sdoApeProf" />
        </div>
        <div class="form-group">
            <label for="teleProfe">Telefono</label>
            <input id="teleProfe" class="form-control" type="text" name="teleProfe" />
        </div>
        <div class="form-group">
            <label for="correoProfe">Correo</label>
            <input id="correoProfe" class="form-control" type="text" name="correoProfe" />
        </div>
        <div class="form-group">
            <input type="submit" class="btn btn-primary pull-right" name="Registrar" value="Registrar">
        </div>
    </form>
</div>

